<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zmg.book.mapper.oraclemapper.UserOracleMapper">
    <select id="getAll" resultType="com.zmg.book.entity.Student">
        SELECT * FROM STUDENT
    </select>

    <select id="selectById" parameterType="long" resultType="com.zmg.book.entity.Student">
        SELECT * FROM STUDENT WHERE ID = #{id}
    </select>

    <select id="selectList" resultType="com.zmg.book.entity.Student">
        SELECT * FROM STUDENT
    </select>

</mapper>